{
  "name": "restaurant-api",
  "version": "0.0.1",
  "description": "Restaurant API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "set NODE_ENV=production pm2 start dist",
    "prestart": "npm run -s build",
    "lint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vtestiler/Restaurant-API.git"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env":{
      "node": true
    },
    "rules":{
      "no-console": 0,
      "no-unuser-vars": 1
    }
  },
  "author": "Vladimir Testiler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vtestiler/Restaurant-API/issues"
  },
  "homepage": "https://github.com/vtestiler/Restaurant-API#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "mongoose": "^5.4.19"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^5.15.3",
    "nodemon": "^1.18.10"
  }
}
